{% extends "layout.jinja2" %}

{% block content %}
		<div class="span10 ">
				<div class="lead pull-left">Choose a target</div>
				<div class="btn btn-primary pull-right disabled set_target">
						Set Target
				</div>
		</div>
		<div class="row">		

				<div class="span12">

						{% for i in frame_count %}
								
								<div class="span2 border adjacent-table-cell">
										<a href="#" class="frame" data-frame={{ i }} 
												data-basename={{ file_basename }}>
												<img src=/{{ gif_dir }}/{{ file_basename }}{{ i }}.png />
										</a>
								</div>

						{% endfor %}
				</div>
		</div>		

{% endblock content %}
