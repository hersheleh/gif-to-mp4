{% extends "layout.jinja2" %}


{% block content %}

		<ul class="span10 nav nav-pills">
				<li class="active choose-target">
				<a class="lead" href="#">Choose a target</a>
		</li>

		<li class="choose-subset">
				<a href="#" class="lead">Choose a subset</a>
		</li>
		<div class="span3 offset1 message" style="font-size:18px">
				
		</div>

				<div class="btn btn-primary pull-right disabled convert">
						Convert
				</div>
		</ul>

		<div class="row">		
				<div class="span12">

						{% for i in frame_count %}
								
								<div class="span2 border adjacent-table-cell">
										<a href="#" class="frame" data-frame={{ i }} 
												data-basename={{ asset_name }}>
												<img src=/{{ frame_path }}/{{ asset_name }}_{{ i }}.png />
										</a>
								</div>

						{% endfor %}
				</div>
		</div>		

{% endblock content %}
